generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model users {
  id Int @id  @default(autoincrement())
  email String @unique
  first_name String
  last_name String
  password String
  image String?
  image_color String?
  created_at DateTime @default(now())
  updated_at DateTime @default(now())
}
